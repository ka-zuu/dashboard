// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Widget {
  id       String @id @default(uuid())
  pluginId String
  x        Int
  y        Int
  w        Int
  h        Int
  settings String // JSON string
}
